# TA Summary Automation Configuration
# Copy this file to .env and update with your actual values

# TA Summary System URL (REQUIRED)
TA_SUMMARY_URL=https://your-domain.com

# PDF Files Directory (optional, defaults to ./pdfs)
PDF_FOLDER=./pdfs

# Browser Configuration
HEADLESS_MODE=false
BROWSER_SLOW_MO=500
DEFAULT_TIMEOUT=30000

# Retry Configuration
MAX_RETRY_ATTEMPTS=3
RETRY_DELAY_MS=2000

# Debug Settings
ENABLE_SCREENSHOTS=true
VERBOSE_LOGGING=true

# Authentication (if needed)
# USERNAME=your-username
# PASSWORD=your-password